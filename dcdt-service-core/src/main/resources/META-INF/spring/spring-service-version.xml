<?xml version="1.0" encoding="UTF-8"?>
<beans:beans
    xmlns:beans="http://www.springframework.org/schema/beans"
    xmlns:c="http://www.springframework.org/schema/c"
    xmlns:dcdt="http://direct-test.com/spring/schema/spring-dcdt"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://direct-test.com/spring/schema/spring-dcdt http://direct-test.com/spring/schema/spring-dcdt.xsd
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <!--====================================================================================================
    = MODULE VERSIONS
    =====================================================================================================-->
    <!--suppress SpringModelInspection -->
    <beans:bean id="toolModuleVersionServiceParent" parent="toolModuleVersionImpl"
        c:groupId="${dcdt.module.dcdt-service-parent.groupId:}"
        c:artifactId="${dcdt.module.dcdt-service-parent.artifactId:}"
        c:version="${dcdt.module.dcdt-service-parent.version:}"
        c:name="${dcdt.module.dcdt-service-parent.name:}"
        c:description="${dcdt.module.dcdt-service-parent.description:}"
        p:buildTimestampString="${dcdt.module.dcdt-service-parent.build.timestamp:}"
        p:gitAuthor="${dcdt.module.dcdt-service-parent.git.author:}"
        p:gitBranch="${dcdt.module.dcdt-service-parent.git.branch:}"
        p:gitCommitId="${dcdt.module.dcdt-service-parent.git.commit.id:}"
        p:gitCommitIdShort="${dcdt.module.dcdt-service-parent.git.commit.id.short:}"
        p:gitCommitTimestampString="${dcdt.module.dcdt-service-parent.git.commit.timestamp:}"
        p:gitUrl="${dcdt.module.dcdt-service-parent.git.url:}"/>
    
    <!--suppress SpringModelInspection -->
    <beans:bean id="toolModuleVersionServiceCore" parent="toolModuleVersionImpl"
        c:groupId="${dcdt.module.dcdt-service-core.groupId:}"
        c:artifactId="${dcdt.module.dcdt-service-core.artifactId:}"
        c:version="${dcdt.module.dcdt-service-core.version:}"
        c:name="${dcdt.module.dcdt-service-core.name:}"
        c:description="${dcdt.module.dcdt-service-core.description:}"
        p:buildTimestampString="${dcdt.module.dcdt-service-core.build.timestamp:}"
        p:gitAuthor="${dcdt.module.dcdt-service-core.git.author:}"
        p:gitBranch="${dcdt.module.dcdt-service-core.git.branch:}"
        p:gitCommitId="${dcdt.module.dcdt-service-core.git.commit.id:}"
        p:gitCommitIdShort="${dcdt.module.dcdt-service-core.git.commit.id.short:}"
        p:gitCommitTimestampString="${dcdt.module.dcdt-service-core.git.commit.timestamp:}"
        p:gitUrl="${dcdt.module.dcdt-service-core.git.url:}"/>
</beans:beans>